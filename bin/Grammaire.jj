  public static Object[][] DataGenerator(Function myFunction) 
  {
    int max =  10000; 
    int min = -10000; 
    int range = max - min + 1;
    
	String type = "";
	
	int qty = myFunction.getParameter_declaration_list().size();
	int ctr = 0;	
