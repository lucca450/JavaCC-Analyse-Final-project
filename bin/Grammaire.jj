    	  int random_int;
    	  for (int i = 0; i < 10; i++)
    	  { 
            	random_int = (int)(Math.random() * range) + min;
				tmpArray[ctr][i] = random_int;
		  }
    	  		
    	  
    	}else if(type.equals("double"))
