            	tmpArray[ctr][i] = random_double;
		  }
    	  
    	}else if(type.equals("bool"))
    	{
    	  for (int i = 0; i < 10; i++)
    	  { 
		    	Random r = new Random();
		    	tmpArray[ctr][i] = r.nextBoolean();
		  }
    	}


    	ctr++;
    }

    return tmpArray;
  }
  
